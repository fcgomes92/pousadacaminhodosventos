{
  "name": "pousadacaminhodosventos.com",
  "version": "2.4.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-scripts start",
    "build": "npm run build-css && react-scripts build",
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
    "precache": "sw-precache --config=config/sw-precache-config.js --verbose",
    "deploy": "npm run build && firebase deploy --only hosting"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:fcgomes92/pousadacaminhodosventos.git"
  },
  "author": "@fcgomes92",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fcgomes92/pousadacaminhodosventos/issues"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "directus-sdk-javascript": "^2.2.0",
    "prop-types": "^15.6.0",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-router-dom": "^4.2.2",
    "react-toolbox": "^2.0.0-beta.12",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "firebase-tools": "^3.13.1",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.1.1",
    "react-scripts": "^1.0.14",
    "sw-precache": "^5.1.0"
  }
